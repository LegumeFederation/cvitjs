  "homepage": "https://github.com/LegumeFederation/cvitjs#readme",
  "dependencies": {
    "paper": "^0.12.3",
    "qs": "^6.9.1",
    "rbush": "^2.0.2",
    "skeleton.css": "^2.0.4"
  },
  "files": [
    "build",
    "lib",
    "src"
  ],
  "bundlesize": [
    {
      "path": "./build/cvit.js"
    }
  ]
