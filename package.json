    "postcss-nested": "^4.1.1",
    "postcss-simple-vars": "^5.0.1",
    "promise-polyfill": "^8.1.0",
    "query-string": "^6.2.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
