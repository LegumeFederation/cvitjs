    "ajv-cli": "^3.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.2.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.13.0",
